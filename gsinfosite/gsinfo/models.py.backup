# 状态表
class status(models.Model):
    serialNumber = models.CharField(max_length = 256)            				# 编号
    boxNumber = models.PositiveIntegerField()                    				# 箱号
    
    status1st = models.PositiveIntegerField(default = 0)         				# 环节1状态: 0:未完成 1:完成
    operator1st = models.CharField(max_length = 512, null = True)   			# 环节1记录员
    createDate1st = models.DateField(auto_now_add = True, auto_now = False)		# 环节1记录生成时间
    UpdateDate1st = models.DateField(auto_now_add = False, auto_now = True)		# 环节1记录最近一次修改时间
    
    status2nd = models.PositiveIntegerField(default = 0)                    				# 环节2状态: 0:未完成 1:完成
    operator2nd = models.CharField(max_length = 512, null = True)   			# 环节2记录员
    createDate2nd = models.DateField(auto_now_add = True, auto_now = False)		# 环节2记录生成时间
    UpdateDate2nd = models.DateField(auto_now_add = False, auto_now = True)		# 环节2记录最近一次修改时间
    
    status3rd = models.PositiveIntegerField(default = 0)                    				# 环节3状态: 0:未完成 1:完成
    operator3rd = models.CharField(max_length = 512, null = True)   			# 环节3记录员
    createDate3rd = models.DateField(auto_now_add = True, auto_now = False)		# 环节3记录生成时间
    UpdateDate3rd = models.DateField(auto_now_add = False, auto_now = True)		# 环节3记录最近一次修改时间
    
# 属性表
class property(models.Model):
    project = models.CharField(max_length = 256)	 			 		# 项目
    type = models.CharField(max_length = 256)		 			 		# 类型
    code = models.CharField(max_length = 256, null = True)				# 代码
	
    parentProject = models.CharField(max_length = 256, null = True)		# 父项目
    parentType = models.CharField(max_length = 256, null = True)		# 父类型
	
    grandpaProject = models.CharField(max_length = 256, null = True)	# 祖项目
    grandpaType = models.CharField(max_length = 256, null = True)		# 祖类型