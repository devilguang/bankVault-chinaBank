# encoding=UTF-8
from django.db import models

# Create your models here.

# 工作空间
class workSpace(models.Model):
    boxNumber = models.PositiveIntegerField()
    productType = models.PositiveIntegerField()

# 金银锭类    
class gsDing(models.Model):
    serialNumber = models.CharField(max_length = 256)            			# 编号
    boxNumber = models.PositiveIntegerField()                   			# 箱号
    productType = models.PositiveIntegerField()				 	 			# 实物类型
    className = models.PositiveIntegerField()                    			# 品名
    subClassName = models.PositiveIntegerField()                 			# 明细品名

    detailedName = models.CharField(max_length = 1024, null = True)         # 名称
    typeName = models.CharField(max_length = 512, null = True)              # 型制类型
    peroid = models.CharField(max_length = 256, null = True)                # 时代
    producerPlace = models.CharField(max_length = 512, null = True)         # 制作地/制作人
    carveName = models.CharField(max_length = 512, null = True)             # 铭文
    remark = models.TextField(default = '')                       			# 备注
    quality = models.CharField(max_length = 256, null = True)               # 品相
    level = models.CharField(max_length = 256, null = True)                 # 评价等级
    originalQuantity = models.FloatField(null = True)                       # 原标注成色
    
    detectedQuantity = models.FloatField(null = True)                       # 检测成色
    
    length = models.FloatField(null = True)                                 # 长度
    width = models.FloatField(null = True)                                  # 宽度
    height = models.FloatField(null = True)                                 # 高度
    grossWeight = models.FloatField(null = True)                            # 毛重
    pureWeight = models.FloatField(null = True)                             # 净重

